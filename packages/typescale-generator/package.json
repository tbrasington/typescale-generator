{
  "name": "@initiate-ui/typescale-generator",
  "version": "1.0.1",
  "description": "Generates typescales in step increments or a with a template",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": "Thomas Brasington<mail@tbrasington.com>",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/figlet": "^1.5.4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "cpy-cli": "^4.1.0",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "tsup": "^6.1.3",
    "typedoc": "^0.23.5",
    "typescript": "^4.7.4"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "format": [
      "esm",
      "cjs",
      "iife"
    ]
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./dist",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest "
  },
  "publishConfig": {
    "access": "public"
  }
}