{
  "name": "turborepo-basic-npm",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build-library": "turbo run build-library",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "test-library": "turbo run test-library"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  }
}